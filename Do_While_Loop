
package dowhileloop;


public class DoWhileLoop {

    public static void main(String[] args) {
      
        int i = 1;
        
        do{
            System.out.println("Bangladesh");
            i++;
        }
        while(i<=5);
        
            
        }
    }
    
/* Output
Bangladesh
Bangladesh
Bangladesh
Bangladesh
Bangladesh
*/





