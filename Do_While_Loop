/*

package dowhileloop;

public class DoWhileLoop {

    public static void main(String[] args) {
      
        int i = 2;
        
        do{
            System.out.println("Bangladesh");
            i = i+2;
        }
        while(i<=10);
        
            
        }
    }

*/
    
/* Output
2
4
6
8
10
*/





