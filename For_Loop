
package forloop;

public class ForLoop {

    public static void main(String[] args) {
        int i;
        for(i=1 ; i<=5; i++){
            System.out.println("Bangladesh");
        }
    }
}

/* Output
Bangladesh
Bangladesh
Bangladesh
Bangladesh
Bangladesh
*/





/*

