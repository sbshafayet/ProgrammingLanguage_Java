
package whileloop;

public class WhileLoop {

    public static void main(String[] args) {
        
        int i = 1;
        
        while(i<=5){
            System.out.println("Bangladesh");
            i++;
        }
    }
    
}

/* Output
Bangladesh
Bangladesh
Bangladesh
Bangladesh
Bangladesh
*/





/*

package whileloop;

public class WhileLoop {

    public static void main(String[] args) {
        
        int i = 1;
        
        while(i<=5){
            System.out.println(i+" Bangladesh");
            i++;
        }
    }
    
}

*/

/* Output
1 Bangladesh
2 Bangladesh
3 Bangladesh
4 Bangladesh
5 Bangladesh
*/





/*

package whileloop;

public class WhileLoop {

    public static void main(String[] args) {
        
        int i = 1;
        
        while(i<=5){
            System.out.println(i);
            i++;
        }
    }
    
}

*/

/* Output
